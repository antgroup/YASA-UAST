{
  "name": "@ant-yasa/uast-spec",
  "version": "0.0.0",
  "description": "uast-spec is a utility tool for uast specification generator and also a node type sanitizer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx gulp build && tsc && cp -rf dist/src/* dist/",
    "generate-type-helpers": "npx gulnpmp generate-type-helpers",
    "generate": "npx gulp generate",
    "gen-spec": "node ./scripts/generators/specification.js > specification.md"
  },
  "dependencies": {
    "@babel/helper-validator-identifier": "^7.16.7",
    "fancy-log": "^2.0.0",
    "help": "^3.0.2",
    "to-fast-properties": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/generator": "^7.17.0",
    "@babel/parser": "^7.17.0",
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@types/node": "^17.0.17",
    "chalk": "^5.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^7.1.7",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^27.4.0",
    "jest-worker": "^27.4.0",
    "prettier": "^2.5.1",
    "through2": "^4.0.2"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "conditions": {
    "BABEL_8_BREAKING": [
      null,
      {
        "exports": null
      }
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/antgroup/YASA-UAST.git"
  
}
